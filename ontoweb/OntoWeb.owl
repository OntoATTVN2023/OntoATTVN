  <identifiedBy rdf:resource="#DS0012_001"/>
  <defenseBy rdf:resource="#D3_DA"/>
  <defenseBy rdf:resource="#D3_FA"/>
  <defenseBy rdf:resource="#D3_FIM"/>
  <defenseBy rdf:resource="#D3_EFA"/>
