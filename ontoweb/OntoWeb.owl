  <defenseBy rdf:resource="#D3_FIM"/>
  <defenseBy rdf:resource="#D3_EFA"/>
  <defenseBy rdf:resource="#D3_DF"/>
  <defenseBy rdf:resource="#D3_FR"/>
  <defenseBy rdf:resource="#D3_FE"/>
