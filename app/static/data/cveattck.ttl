###  http://test.org/Ontology.owl#T1046
:T1046 rdf:type owl:NamedIndividual ,
                :Technique ;
       :accomplishedTactic :TA0007 ;
       :identifiedBy :DS0017_001 ,
                     :DS0025_002 ,
                     :DS0029_003 ;
       :preventedBy :M1030 ,
